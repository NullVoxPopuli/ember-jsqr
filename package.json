{
  "name": "ember-jsqr",
  "version": "2.0.0",
  "description": "ember integration with jsQR - a very fast QR Code scanner",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/NullVoxPopuli/ember-jsqr",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:try-one": "ember try:one",
    "test:all": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "ember-auto-import": "^2.2.4",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "^4.2.1",
    "ember-modifier": "^3.0.0",
    "jsqr": "^1.4.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.2",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@nullvoxpopuli/eslint-configs": "^2.1.12",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__test-helpers": "^2.6.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~4.0.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.0.1",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^2.0.0",
    "semantic-release": "^18.0.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0"
  },
  "engines": {
    "node": "14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://nullvoxpopuli.github.io/ember-jsqr/"
  },
  "homepage": "https://NullVoxPopuli.github.io/ember-jsqr",
  "volta": {
    "node": "14.18.2",
    "yarn": "1.22.17"
  }
}
